DROP TABLE IF EXISTS table_a;

CREATE TABLE table_a (foo INT, bar STRING) COMMENT 'table to test DDL ops' PARTITIONED BY (ds STRING) STORED AS TEXTFILE;

LOAD DATA LOCAL INPATH '[[EXAMPLES]]/kv1.txt' OVERWRITE INTO TABLE table_a PARTITION (ds='2008-08-15');
LOAD DATA LOCAL INPATH '[[EXAMPLES]]/kv2.txt' OVERWRITE INTO TABLE table_a PARTITION (ds='2008-08-08');

SELECT ds, COUNT(*) FROM table_a GROUP BY ds;

DROP TABLE IF EXISTS table_a;